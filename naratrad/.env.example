# Supabase Database Configuration
# Copy this file to .env and fill in your Supabase credentials

# Supabase PostgreSQL Connection
SUPABASE_DB_URL=jdbc:postgresql://<your-project-ref>.supabase.co:5432/postgres
SUPABASE_DB_USERNAME=postgres
SUPABASE_DB_PASSWORD=<your-database-password>

# Finnhub API
FINNHUB_API_KEY=<your-finnhub-api-key>

# Application Port (optional)
SERVER_PORT=8080

MAIL_HOST=sandbox.smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=<your_mailtrap_username>
MAIL_PASSWORD=<your_mailtrap_password>
MAILTRAP_API_TOKEN=your-token-from-mailtrap-api-settings
MAILTRAP_INBOX_ID=your-mailtrap-inbox